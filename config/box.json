{
  "directories": [
    "bin",
    "src"
  ],
  "files": [
    "constants.php",
    "vendor/autoload.php",
    "config/config.json",
    "config/services.php",
    "shell-config-bash.rc",
    "shell-config.rc"
  ],
  "finder": [
    {
      "in": "vendor",
      "name": "*.php*",
      "exclude": [
        "phpunit",
        "Tests",
        "tests"
      ]
    }
  ],
  "stub": "stub.php",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Json",
    "KevinGH\\Box\\Compactor\\Php"
  ]
}